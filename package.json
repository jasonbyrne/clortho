{
  "name": "clortho",
  "version": "1.3.2",
  "description": "friendly, OS-appropriate password handling for node scripts",
  "main": "lib/index.js",
  "types": "index.d.ts",
  "scripts": {
    "babel": "babel src -d lib",
    "lint": "semistandard **/*.js",
    "test": "npm run lint && npm run babel",
    "preversion": "npm run test",
    "version": "git add -A ."
  },
  "semistandard": {
    "parser": "babel-eslint",
    "ignore": "lib/**"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonbyrne/clortho"
  },
  "keywords": [
    "keychain",
    "password"
  ],
  "license": "ISC",
  "dependencies": {
    "js-string-escape": "^1.0.0",
    "keychain": "^1.0.0",
    "which": "^1.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "estraverse-fb": "^1.3.2",
    "semistandard": "*"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
